model Service {
    id            String          @id @default(cuid())
    title         String
    description   String
    tags          String[]
    startingPrice Float?
    image         String?
    category      ServiceCategory

    walkIns WalkIn[]

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

enum ServiceCategory {
    Printing
    Repair
}
