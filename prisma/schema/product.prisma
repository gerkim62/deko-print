model Product {
    id          String          @id
    title       String
    description String
    tags        String[]
    price       Float
    image       String?
    category    ProductCategory

    orders  Order[]
    walkIns WalkIn[]

    stockRemaining Int

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

enum ProductCategory {
    Pre_owned
    Accessory
}
